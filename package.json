{
  "devDependencies": {
    "rollup": "^2.79.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-virtual": "^3.0.0",
    "@rollup/plugin-inject": "^5.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "rollup-plugin-executable": "^1.6.3",
    "rollup-plugin-native": "^1.2.16",
    "rollup-plugin-consts": "^1.1.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-gzip": "^3.0.1"
  },
  "template": {
    "properties": {
      "rollup": {
        "config": ""
      },
      "testcafe": {
        "app": "\"rollup -c {{rollup.config}} -w\""
      }
    },
    "inheritFrom": [
      "arlac77/template-javascript"
    ],
    "usedBy": [
      "arlac77/template-svelte-app",
      "arlac77/template-svelte-component",
      "arlac77/template-vue-app",
      "konsumation/konsum"
    ],
    "mergers": [
      {
        "type": "Rollup",
        "pattern": "**/rollup.config.*"
      }
    ]
  }
}
